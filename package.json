{
  "name": "setup-react-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=16.0.0",
    "yarn": ">=1.22.0",
    "npm": "please-use-yarn"
  },
  "scripts": {
    "dev": "vite",
    "test": "jest",
    "lint:check": "eslint .",
